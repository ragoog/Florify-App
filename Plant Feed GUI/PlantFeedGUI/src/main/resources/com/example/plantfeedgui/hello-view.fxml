<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="616.0" prefWidth="570.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/25" fx:controller="com.example.plantfeedgui.FeedPageController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
   <children>
      <Label alignment="CENTER" prefHeight="31.0" prefWidth="137.0" text="Plant Feed" textAlignment="CENTER">
         <font>
            <Font name="Arial Bold" size="24.0" />
         </font>
      </Label>
      <ScrollPane prefHeight="515.0" prefWidth="550.0">
         <content>
            <VBox fx:id="PostsContainer" prefHeight="471.0" prefWidth="511.0" />
         </content>
      </ScrollPane>
      <HBox alignment="TOP_CENTER" prefHeight="98.0" prefWidth="550.0">
         <children>
            <TextField fx:id="postTextField" alignment="CENTER" prefHeight="37.0" prefWidth="481.0" promptText="What's on your mind?.." />
            <Button fx:id="postButton" mnemonicParsing="false" prefHeight="35.0" prefWidth="51.0" text="Post" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
